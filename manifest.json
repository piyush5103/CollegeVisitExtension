{
	"manifest_version": 2,
	"name": "College2Calendar",
	"description": "Add admission events directly to Google Calendar.",
	"version": "0.1.0",
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "College2Calendar"
	},
	"icons": {
		"512": "logo-new.png"
	}
}
